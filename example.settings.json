{
  "global_settings": {
    "default_network_interface": "tun0",
    "logger_level": "info",
    "disable_color": false,
    "used_module": [
      "session",
      "common"
    ],
    "wordlist_base": "~/oscp-swiss/wordlist"
  },
  "modules": {
    "session": {
      "default_username_wordlist": "/usr/share/wordlists/seclists/Usernames/top-usernames-shortlist.txt",
      "default_password_wordlist": "/usr/share/wordlists/seclists/Passwords/xato-net-10-million-passwords-100.txt",
      "sticky_session": true
    },
    "alias": {
      "wpscan_token": "",
      "use_custom_xfreerdp": false,
      "use_custom_wpscan": true,
      "use_custom_cat": false
    },
    "recon": {
      "directory_recursive_depth": 1,
      "directory_busting_wordlist": "~/oscp-swiss/wordlist/ffuf-default.txt",
      "file_traversal_wordlist": "~/oscp-swiss/wordlist/file-traversal-default.txt",
      "subdomain_wordlist": "~/oscp-swiss/wordlist/subdomain+vhost-default.txt",
      "vhost_wordlist": "~/oscp-swiss/wordlist/subdomain+vhost-default.txt"
    }
  },
  "variable": {
    "used_session": ""
  }
}
